{
  "name": "full-monty",
  "version": "4.29.0",
  "private": true,
  "description": "This is the new e-commerce project",
  "main": "index.js",
  "scripts": {
    "prelerna:build": "lerna run clean",
    "lerna:build": "lerna run build",
    "lerna:clean": "lerna run clean && lerna clean --yes",
    "prelerna:dev": "lerna run clean",
    "lerna:dev": "lerna exec -- npm run dev",
    "lerna:test": "lerna exec -- npm run test",
    "lerna:outdated": "lerna --concurrency 1 exec -- ncu -e 2 --packageFile ./package.json",
    "lerna:update": "lerna --concurrency 1 exec -- ncu -a --packageFile ./package.json",
    "lerna:install": "lerna bootstrap && npm run lerna:build",
    "start": "node --harmony index.js",
    "build": "BABEL_ENV=production node --max-old-space-size=8192 node_modules/webpack/bin/webpack.js --color --config build/webpack/prod.config.js --bail",
    "build:dev": "node --max-old-space-size=8192 node_modules/webpack/bin/webpack.js  --color --config build/webpack/dev.config.js --bail",
    "build:analyse": "node --max-old-space-size=8192 node_modules/webpack/bin/webpack.js --color --config build/webpack/analyse.config.js --bail",
    "cache": "docker-compose up -d monty_cache",
    "dev": "npm run cache; ./scripts/write_version_info_file.sh; npm run web-dev-server & ./scripts/wait_for_critical_css.sh && npm run server ",
    "dev-delete-critical": "npm run cache; ./scripts/write_version_info_file.sh; npm run web-dev-server & ./scripts/wait_for_critical_css.sh DELETE_CRITICAL && npm run server ",
    "web-dev-server": "node --max-old-space-size=8192 --harmony -r  dotenv/config ./node_modules/.bin/webpack-dev-server --debug --output-pathinfo --config build/webpack/dev.config.js",
    "dev:debugger": "NODE_OPTS=\"${NODE_OPTS} --inspect=${NODE_DEBUG_PORT:-9229}\" WEBSERVER_NODE_OPTS=\"${WEBSERVER_NODE_OPTS} --inspect=${WEBSERVER_NODE_DEBUG_PORT:-9230}\" npm run dev",
    "server": "USE_COMMONJS=true ./node_modules/.bin/supervisor --instant-kill -w 'nothing' -- --harmony -r dotenv/config ${NODE_OPTS} .",
    "test": "npm run lint && npm run unit:test && npm run integration:test && npm run e2e:test",
    "lint": "echo \"Linting....\"; npm run lint-src && npm run lint-integration && npm run e2e:lint",
    "lint-src": "eslint --rulesdir=build/eslint-rules/ \"src/**/*.js\" \"src/**/*.jsx\"",
    "lint-unit": "eslint --rulesdir=build/eslint-rules/ \"test/unit/**/*.js\" \"test/unit/**/*.jsx\"",
    "lint-integration": "eslint --rulesdir=build/eslint-rules/ \"test/integration/**/*.js\" \"test/integration/**/*.jsx\"",
    "unit:test": "echo \"Unit tests....\"; npm run jest",
    "e2e": "CI=true NODE_ENV=production npm run build && npm run server",
    "e2e:clean": "rm -rf test/e2e/logs/*.log test/e2e/screenshots/*.png test/reports/bdd/*.json",
    "e2e:lint": "eslint --rulesdir=build/eslint-rules/  \"test/e2e/**/*.js\"",
    "e2e:test": "npm run e2e:clean; echo \"E2E tests....\" && wdio test/e2e/wdio.config.js",
    "be2e:test": "npm run e2e:clean; echo \"E2E tests on BrowserStack....\" && wdio test/e2e/bs.wdio.config.js",
    "e2e:test:mobile": "BREAKPOINT=mobileM npm run e2e:test",
    "e2e:test:desktop": "BREAKPOINT=desktop npm run e2e:test",
    "e2ev2": "echo \"v2 build starting\"; wdio test/e2ev2/wdio.conf.js | ./node_modules/.bin/bunyan -o short",
    "e2ev2:lint": "eslint --rulesdir=build/eslint-rules/  \"test/e2ev2/**/*.js\"",
    "e2ev2:desktop": "npm run e2ev2:lint && BREAKPOINT=desktop npm run e2ev2",
    "e2ev2:mobile": "npm run e2ev2:lint && BREAKPOINT=mobileM npm run e2ev2",
    "functional:cli": "node ./test/functional/cli/run-functional-tests.js",
    "functional": "CORE_API_PORT=4000 FUNCTIONAL_TESTS=true QUBIT_DISABLED=true npm run server & node ./test/functional/mock-server/index.js & MONTY_PORT=3000 npm run functional:cypress:open",
    "functional:dev": "CORE_API_PORT=4000 FUNCTIONAL_TESTS=true QUBIT_DISABLED=true npm run dev & node ./test/functional/mock-server/index.js & npm run functional:cypress:open",
    "functional:mobile": "BREAKPOINT=mobile npm run functional",
    "functional:desktop": "npm run functional",
    "functional:fromcli": "CORE_API_PORT=4000 FUNCTIONAL_TESTS=true QUBIT_DISABLED=true npm run server & node ./test/functional/mock-server/index.js & MONTY_PORT=3000 ./test/functional/wait-for-monty && MONTY_PORT=3000 cypress run --project test/functional --reporter ../../node_modules/mocha-multi-reporters --reporter-options configFile=mochaReporters.json",
    "functional:fromcli:mobile": "BREAKPOINT=mobile npm run functional:fromcli ",
    "functional:cypress:test": "cypress run --project --reporter mochawesome'",
    "functional:cypress:run": "cypress run --project test/functional",
    "functional:cypress:open": "cypress open --project test/functional",
    "functional:lint": "eslint \"**/*.js\"",
    "functional:precommit": "lint-staged",
    "functional:cy:run": "node scripts/run.js",
    "unit:coverage": "npm run jest:coverage",
    "jest:no-opts": "npm run jest",
    "jest": "jest --maxWorkers 3 --config=./build/unit/jest.config.json",
    "jest:fast": "jest --config=./build/unit/jest.config.json",
    "jest:only": "jest",
    "jest:watch": "npm run jest -- --watch -o",
    "jest:updateSnapshots": "npm run jest -- -u",
    "jest:coverage": "npm run jest -- --coverage",
    "integration:coreapi": "jest --config=./build/integration/jest.config.js ./test/apiResponses/coreapi/*/*.spec.js",
    "int:coreapi:watch": "jest --config=./build/integration/jest.config.js --watch",
    "integration:validate-third-parties": "jest --config=./build/integration/jest.config.js test/apiResponses/third-parties/*/*.spec.js",
    "npm-shrinkwrap": "rm -Rf npm-shrinkwrap.json node_modules && npm i && npm shrinkwrap --dev",
    "validate": "npm ls",
    "prettier": "prettier \"test/**/*.js\" \"test/**/*.jsx\" \"src/**/*.js\" \"src/**/*.jsx\" --write",
    "precommit": "./scripts/npm-version-check.sh && lint-staged",
    "postmerge": "npm run features:fetch",
    "commitmsg": "node .githooks/prepare-commit-msg/prepend-branch-id",
    "prepush": "npm run unit:test",
    "postinstall": "(cd ./test/functional/mock-server && npm install)",
    "generate-api-docs": "node --harmony -r dotenv/config scripts/generate_swagger_spec",
    "features:checkout": "./scripts/features_checkout.sh",
    "features:fetch": "./scripts/features_fetch.sh",
    "features:push": "./scripts/features_push.sh",
    "rfc:analyse": "rm -f $INIT_CWD/results.log && jscodeshift --extensions js,jsx --transform $INIT_CWD/analyse.js ./src/",
    "rfc:process": "node $INIT_CWD/process-results.js",
    "rfc:transform": "jscodeshift --extensions js,jsx --transform $INIT_CWD/transform.js ./src/",
    "create-release-candidate": "./scripts/npm-version-check.sh && ./scripts/create_release_candidate.sh"
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx}": [
        "prettier --write",
        "eslint --rulesdir=build/eslint-rules/",
        "git add"
      ],
      "*.json": [
        "prettier --write",
        "git add"
      ]
    }
  },
